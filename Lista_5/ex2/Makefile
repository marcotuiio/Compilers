EXEC = gcc
CFLAGS = -fstack-protector-all -g -Wall
PROJECT_NAME = l5e2
OBJETCS = LISTA_5_EXERCICIO_2.o

clear: 
	rm -f *.o
	rm -f $(PROJECT_NAME)

$(PROJECT_NAME): $(OBJETCS)
	$(EXEC) $(CFLAGS) $(OBJETCS) -o $(PROJECT_NAME) -lm

LISTA_5_EXERCICIO_2.o: LISTA_5_EXERCICIO_2.c LISTA_5_EXERCICIO_2.h
	$(EXEC) $(CFLAGS) -c LISTA_5_EXERCICIO_2.c